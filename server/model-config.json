{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "firestore",
    "acls": [
      {
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "ALLOW"
      }
    ]
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "wod": {
    "dataSource": "firestore",
    "public": true
  },
  "Resources": {
    "dataSource": "firestore",
    "public": true
  },
  "leaderboard": {
    "dataSource": "firestore",
    "public": true
  },
  "gamesEvents": {
    "dataSource": "firestore",
    "public": true
  },
  "metadata": {
    "dataSource": "firestore",
    "public": true
  },
  "Email": {
    "dataSource": "db"
  },
  "score": {
    "dataSource": "firestore",
    "public": true
  }
}
